                <icon>trash</icon>
                <url>https://data.townofcary.org/api/records/1.0/search/?dataset=solid-waste-and-recycling-collection-routes&amp;geofilter.distance=${project.latitude},${project.longitude},0&amp;rows=1&amp;fields=day,cycle&amp;location=14,35.82836,-78.79201</url>
                <fields>day=Pickup Day,cycle=Recycle Week</fields>
                <display>details</display>
              </portlet>
            </column>
          </row>
          <row>
            <column class="medium-6">
              <portlet name="OpenDataSoftPortlet" viewer="dataTable">
                <title>Building data</title>
                <icon>building</icon>
                <url>https://data.townofcary.org/api/records/1.0/search/?dataset=building-points&amp;q=location%3D%22${project.title:url}%22&amp;rows=1</url>
                <fields>yearbuilt=Year built,sqft=Square feet,bldgstyle=Building style,owner=Owner,owner2=Owner,units=Number of units,rooms=Number of rooms,storyheight=Number of stories</fields>
                <display>details</display>
              </portlet>
            </column>
            <column class="medium-6">
              <portlet name="OpenDataSoftPortlet" viewer="dataTable">
                <title>Real estate data</title>
                <icon>map</icon>
                <url>https://data.townofcary.org/api/records/1.0/search/?dataset=property&amp;fields=county,deedacres,bldgvalue,landvalue,totalsaledate,totalsalevalue&amp;q=location%3D%22${project.title:url}%22&amp;rows=1</url>
