                <fields>application=Permit,application_type=Type,structure_details=Details</fields>
                <unused>general_contractor=Contractor</unused>
              </portlet>
              <portlet name="OpenDataSoftPortlet" viewer="dataTable">
                <title>Contractors at this location</title>
                <icon>truck</icon>
                <url>https://data.townofcary.org/api/records/1.0/search/?dataset=permit-contractors&amp;fields=year,contractorcompanyname2&amp;q=applicationaddress1%3D%22${project.title:url}%22&amp;rows=100&amp;sort=-year&amp;facet=applicationaddress1</url>
                <fields>year=Year,contractorcompanyname2=Contractor</fields>
              </portlet>
              <portlet name="OpenDataSoftPortlet" viewer="dataTable">
                <title>Recent crime nearby</title>
                <icon>user-secret</icon>
