<module>
  <navigation>

    <page name="project profile: places">
      <row>
        <column class="medium-7 ">
          <row>
            <column class="medium-8">
              <portlet name="ProfileDescriptionPortlet" class="profile-portlet-body ">
                <showTitle>true</showTitle>
                <title>Description</title>
                <showInformation>false</showInformation>
                <showShortDescription>true</showShortDescription>
                <showDescription>false</showDescription>
                <editDescriptionText>Update Info</editDescriptionText>
                <showEditDescription>true</showEditDescription>
              </portlet>
            </column>
            <column class="medium-4">
              <!-- Fan Count -->
              <portlet name="ProfileStatsPortlet" class="text-center">
                <title>Fan</title>
                <plural>Fans</plural>
                <icon>thumbs-o-up</icon>
                <type>profileUserCount</type>
              </portlet>
              <portlet name="ProjectActionsPortlet">
                <style>links</style>
                <urls>
                  <value><![CDATA[
                    <content>Become a Fan</content>
                    <title>Join ${project.title:xml} as a fan</title>
                    <class>button small radius expand</class>
                    <href>/ProjectManagementTeam.do?command=ConfirmJoin&amp;pid=${project.id}&amp;type=fan</href>
                    <class>button small radius expand</class>
                    <rel>shadowbox;width=600</rel>
                    <rule>userCanJoin</rule>]]>
                  </value>
                  <value><![CDATA[
                    <content>Become a Fan</content>
                    <title>Request to join ${project.title:xml} as a member</title>
                    <class>button small radius expand</class>
                    <href>/ProjectManagementTeam.do?command=ConfirmAskToBecomeMember&amp;pid=${project.id}</href>
                    <rel>shadowbox;width=600</rel>
                    <rule>userCanRequestToJoin</rule>]]>
                  </value>
                </urls>
              </portlet>
            </column>
          </row>
          <row>
            <column class="small-12">
              <portlet name="UpcomingEventListPortlet" class="detailed-list ">
                <title>Upcoming Events</title>
                <limit>5</limit>
                <successMessage>You have successfully joined this event!</successMessage>
              </portlet>
            </column>
          </row>
          <row>
            <column class="small-12 medium-4 ">
              <portlet name="ProjectActionsPortlet">
                <style>links</style>
                <urls>
                  <value>
                    <![CDATA[<content>Write a Review</content>
                  <title>Review ${project.title:xml}</title>
                  <icon>star-o</icon>
                  <class>button radius expand</class>
                  <href>/create/${project.uniqueId}/review?redirectTo=/show/${project.uniqueId}</href>
                  <rel>shadowbox;width=600</rel>
                  <permission>project-reviews-add</permission>
                  <rule>userCanReview</rule>]]>
                  </value>
                  <value>
                    <![CDATA[<content>Write a Review</content>
                  <title>Review ${project.title:xml}</title>
                  <icon>star-o</icon>
                  <class>button radius expand</class>
                  <href>/page/registration-required</href>
                  <rel>shadowbox;width=600</rel>
                  <rule>isNotUser</rule>]]>
                  </value>
                </urls>
              </portlet>
            </column>
          </row>
          <row>
            <column class="small-12 ">
              <!-- Reviews -->
              <portlet name="RecentProjectReviewsPortlet">
                <!--<title>Recent Reviews</title>-->
                <!--<icon></icon>-->
                <view>verbose</view>
                <showIfEmpty>true</showIfEmpty>
                <showAuthor>true</showAuthor>
                <limit>5</limit>
              </portlet>
            </column>
          </row>
        </column>
        <column class="medium-5 ">
          <!-- Map -->
          <portlet name="ProjectGoogleMapsPortlet" class="panel radius">
            <useShapes>true</useShapes>
            <showMarkers>true</showMarkers>
            <zoomLevel>14</zoomLevel>
          </portlet>
          <!-- Photos -->
          <portlet name="RecentPhotosListPortlet">
            <title>Photos</title>
            <icon>camera-retro</icon>
            <limit>8</limit>
            <showPhotoUpload>true</showPhotoUpload>
            <emptyMessage>There are no photos yet.</emptyMessage>
            <showMore>true</showMore>
          </portlet>
          <!-- Tips -->
          <portlet name="ListsByProjectPortlet">
            <title>Tips</title>
            <icon>heart</icon>
            <list>Tips</list>
            <showListTitle>false</showListTitle>
            <listType>ol</listType>
            <listClass />
            <limit>10</limit>
            <showIfEmpty>false</showIfEmpty>
          </portlet>
          <!-- Hours -->
          <portlet name="ProfileDescriptionPortlet">
            <showTitle>true</showTitle>
            <title>Hours</title>
            <icon>calendar</icon>
            <showInformation>false</showInformation>
            <showShortDescription>false</showShortDescription>
            <showDescription>false</showDescription>
            <wikiForm>Hours</wikiForm>
            <wikiName>Hours</wikiName>
            <wikiAdminText>Please take a moment to update the hours</wikiAdminText>
            <wikiEditText>Edit Hours</wikiEditText>
            <listClass>ccp-eventlist-simple</listClass>
          </portlet>
        </column>
      </row>
    </page>

    <page name="project profile: properties">
